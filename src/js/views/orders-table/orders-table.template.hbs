<table class="table table-striped table-hover import-table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Customer name</th>
        <th scope="col">Amount of products</th>
        <th scope="col">Order volume</th>
        <th scope="col">SKU</th>
    </tr>
    </thead>
    <tbody>
    {{#each orders}}
    <tr class="import-item" data-id="{{id}}">
        <th scope="row">#{{id}}</th>
        <td>{{customer}}</td>
        <td>{{products.length}}</td>
        <td>{{volume}}</td>
        <td>{{sku}}</td>
    </tr>
    {{~/each}}
    </tbody>
</table>